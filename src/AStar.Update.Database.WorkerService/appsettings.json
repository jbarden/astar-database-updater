{
  "Serilog": {
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithSpan",
      "WithExceptionDetails"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System.Net.Http": "Warning",
        "AStar": "Debug"
      }
    }
  },
  "Logging": {
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "DirectoriesList": {
    "Directories": [
      "dir1",
      "dir2"
    ]
  },
  "directoryChanges": {
    "directories": [
    ]
  },
  "apiConfiguration": {
    "filesApiConfiguration": {
      "baseUrl": "https://localhost:7138/"
    },
    "imagesApiConfiguration": {
      "baseUrl": "https://localhost:7008/"
    },
    "directories": [
